# Configuração do Banco de Dados
# Exemplo para desenvolvimento local:
# DATABASE_URL="postgresql://usuario:senha@localhost:5432/nome_banco?schema=public"
# Exemplo para produção (ajuste conforme seu ambiente):
DATABASE_URL="postgresql://app:senha@servidor:5432/banco_producao?schema=public"
# Porta do servidor
PORT=3333

# Configuração de Processamento de Espelhos
# Quantidade de meses para processar ao gerar espelhos diários (padrão: 2)
# Valores recomendados: 1-12 meses
MESES_PROCESSAR=6